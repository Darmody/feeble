{
  "name": "tuku",
  "version": "0.0.1-dev",
  "description": "React + Redux Architecture",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src test",
    "test": "NODE_PATH=src NODE_ENV=test ava",
    "test:examples": "babel-node examples/testAll.js",
    "check:src": "npm run lint && npm run test",
    "check:examples": "npm run build:examples && npm run test:examples",
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "build:examples": "babel-node examples/buildAll.js"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup.js"
    ],
    "files": [
      "test/**/*.spec.js"
    ]
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Wei Zhu <yesmeck@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.1",
    "lodash": "^4.13.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "jsdom": "^9.4.1",
    "react": "^15.2.1",
    "react-router": "^2.6.0",
    "rimraf": "^2.5.4"
  },
  "peerDependencirs": {
    "react": "^0.14.0 || ^15.0.0-0"
  }
}
